<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kinde.springboot.mapper.DrugMapper">

    <select id="findDrug" resultType="java.lang.Integer">
        select number from sys_drug where id = #{drugId};
    </select>
</mapper>